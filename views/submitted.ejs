<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Quizzical</title>
    <link rel="stylesheet" href="styles/submitted.css">
    <link rel="stylesheet" href="styles/search.css">
</head>
<body>
    <div class="top-bar">
        <a href="/"><img src="img/logo.svg" alt="Logo" id="logo"></a>
        <div id="search-bar">
            <input type="text" id="search-box" placeholder="Search users or quizzes">
            <div id="suggestions-box">
                <ul id="suggestions-list">
                </ul>
            </div>
        </div>
    </div>
    
    <div class="sidebar">
        <ul class="list">
            <a href="/create"><li>Create Quiz</li></a>
            <a href=""><li>Friends</li></a>
            <a href="/scores"><li>Past Quizzes</li></a>
            <a href="/user?username=<%= username %>"><li>Profile</li></a>
            <a href="/auth?state=logout"><li>Logout</li></a>
        </ul>
    </div>
    
    <div class="content">
        
        <h1 class="quiz-name"><%= quizInfo[0].quizName %></h1>
        <h3>Your Results</h3>
        <h4 id="score"></h4>
        <div id="display-box"></div>
        <!-- <p>username</p>
        <a href="">Your friends</a>
        <h2>Quizzes you made:</h2>
        <div id="display-box">
            <div class="card">
                <a href="">Quiz 1</a>
            </div>
        </div>  -->
    </div>
    <script>
        let usersName = '<%= usersName %>';
        let username = '<%= username %>';
        let quiz = '<%- JSON.stringify(quiz) %>';
        let userAnswers = '<%- JSON.stringify(userAnswers) %>';
        let quizInfo = '<%- JSON.stringify(quizInfo) %>';
        let score = '<%- score %>';
        let quizzesInfo = '<%- JSON.stringify(quizzesInfo) %>';
        let usersInfo = '<%- JSON.stringify(usersInfo) %>';
    </script>
    <script src="scripts/submitted.js"></script>
    <script src="scripts/search.js"></script>
</body>
</html>